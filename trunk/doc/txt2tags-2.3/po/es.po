#, fuzzy
msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr ""
"  -t, --target       fija el formato de documento final. apoyado actualmente:"

msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr ""
"  -i, --infile=FILE   fija el ARCHIVO como el archivo fuente ('-' para STDIN)"

msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr ""
"  -o, --outfile=FILE  fija el ARCHIVO como el archivo de la salida ('-' para "
"STDOUT)"

msgid "  -n, --enum-title    enumerate all title lines as 1, 1.1, 1.1.1, etc"
msgstr ""
"  -n, --enum-title    enumere todas las líneas del título como 1, 1.1, "
"1.1.1, etc"

msgid "  -H, --no-headers    suppress header, title and footer contents"
msgstr ""
"  -H, --no-headers    suprima la cabeceras, el título y pie de la página"

msgid ""
"      --headers       show header, title and footer contents (default ON)"
msgstr ""
"      --headers       Muestra la cabecera, el título y el contenido del pie "
"de la página (predef.activado)"

#, fuzzy
msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr ""
"      --encoding      codifica el archivo de destinación (utf-8, iso-8859-1, "
"etc)"

msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr ""
"      --style=FILE    usa ARCHIVO como estilo del documento  (como HTML CSS)"

msgid ""
"      --css-sugar     insert CSS-friendly tags for HTML and XHTML targets"
msgstr ""
"      --css-sugar     inserta tag compatible con CSS, para HTML y XHTML"

#, fuzzy
msgid ""
"      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr ""
"      --css-sugar     inserta tag compatible con CSS, para HTML y XHTML"

msgid ""
"      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr "      --mask-email    email anti spam robot. x@y.z vueltas <x (a) y z>"

msgid "      --toc           add TOC (Table of Contents) to target document"
msgstr ""
"      --toc           agregue el TOC (Table of Contents) al documento de la "
"blanco"

msgid "      --toc-only      print document TOC and exit"
msgstr "      --toc-only      imprime índice (TOC) del documento y sale"

msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   fije el nivel máximo del TOC (profundidad) a N"

msgid "  -C, --config-file=F read config from file F"
msgstr ""

